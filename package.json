{
  "name": "gitinspector-csv",
  "version": "1.2.1",
  "description": "Simple module to parse gitinspector XML output and output a CSV file",
  "main": "./src/index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha ./test/*",
    "test-with-cov": "./node_modules/istanbul/lib/cli.js cover --include-all-sources --root ./src ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "lint": "./node_modules/eslint/bin/eslint.js src/*.js bin/*.js",
    "preversion": "npm test",
    "version": "git add -A",
    "postversion": "git push && git push --tags"
  },
  "bin": {
    "gitinspector-csv": "./bin/cli.js"
  },
  "keywords": [
    "gitinspector",
    "xml",
    "csv"
  ],
  "author": {
    "name": "Peter Reid",
    "email": "peter@reidweb.com",
    "url": "https://reidweb.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ReidWeb/GitInspector-CSV"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "commander": "^2.9.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.4.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.3.0",
    "rewire": "^2.5.2"
  }
}
